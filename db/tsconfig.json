{
    "compilerOptions": {
      "target": "ES2022",
      "module": "NodeNext",
      "moduleResolution": "NodeNext",
      "lib": ["ES2022"],
      "esModuleInterop": true,
      "allowSyntheticDefaultImports": true,
      "resolveJsonModule": true,
      "strict": true,
      "skipLibCheck": true,
      "noEmit": true,
      "allowImportingTsExtensions": true,
      "types": ["node"],
    },
    "include": ["**/*.ts"],
  }
  